<?xml version="1.0"?>

<launch>
  
  <arg name="use_camera_control" default="false" />
  <arg name="world_name" default="$(find dyros_simulator)/worlds/parking_lot.world" />
  <arg name="sim_param_file" default="$(find dyros_simulator)/yaml/dyros.yaml" />
  <arg name="urdf_file" default="$(find dyros_simulator)/urdf/mkz.urdf.xacro" />

  
  <include file="$(find dyros_simulator)/launch/gazebo_world.launch" >
    <arg name="world_name" value="$(arg world_name)" />
    <arg name="use_camera_control" value="$(arg use_camera_control)" />
  </include>

  
  <node pkg="dbw_mkz_gazebo" type="mkz_spawner" name="mkz_spawner" output="screen" >
    <param name="sim_param_file" value="$(arg sim_param_file)" />
    <param name="urdf_file" value="$(arg urdf_file)" />
  </node>
  
</launch>
